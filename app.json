{
  "expo": {
    "name": "HungryHub",
    "slug": "HungryHub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/hungryhub_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash_screen.png",
      "backgroundColor": "#294C25"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sourav_ramesh.HungryHub"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "323e328f-5104-4f9e-a68d-c9cb48e35103"
      }
    }
  }
}
